VERSION 3.1.0

INCLUDE preamble.tex
CONFIG
INCLUDE computeRole.py

IMPORT
BEGIN
PARSE personalInfo
PARSE education

if master:
  PARSE employment
else:
  PARSE employment after_date="2020-01-01"

if ml:
    if academic or master:
        PARSE publications
    else:
        PARSE publications latest_k=5
    PARSE funding
    PARSE service

PARSE honors

if academic:
  PARSE talks

if master:
    PARSE projects
elif not academic:
    PARSE projects latest_k=5

PARSE skills
