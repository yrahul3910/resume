VERSION 3.1.0

INCLUDE preamble.tex
CONFIG
INCLUDE computeRole.py

IMPORT
BEGIN
PARSE personalInfo
PARSE education

if not master:
  PARSE employment after_date="2020-01-01"
else:
  PARSE employment

if ml:
    if not academic and not master:
        PARSE publications latest_k=5
    else:
        PARSE publications
    PARSE funding
    PARSE service

PARSE honors

if academic:
  PARSE talks

if not academic and not master:
    PARSE projects latest_k=5
elif not academic and master:
    PARSE projects

PARSE skills
